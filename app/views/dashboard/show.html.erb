<div id='conversational-form'>
  <div class='history'></div>
  <div class='staging'></div>
  <div class='current'></div>
</div>

<%= link_to 'Logout', sessions_path, method: :delete, data: { confirm: 'Are you sure you want to logout?' } %>

<section class="jumbotron  text-center">
  <h1>Welcome, <%= @user['info']['name'] %></h1>
</section>
<section class="container">
  <div class="panel panel-default">
    <div class="panel-body">
      <pre><%= JSON.pretty_generate(@user) %></pre>
    </div>
  </div>
</section>
